<body>
<table>
    <tr>
        <th>Id</th>
        <th>Id Commande</th>
        <th>Nom</th>
        <th>Prenom</th>
        <th>Adresse</th>
        <th>Numero</th>
    </tr>

    <tr {% for p in repo %}>
        <td>{{ p.id }}</td>
        <td>{{ p.idcommande }}</td>
        <td>{{ p.nom }}</td>
        <td>{{ p.prenom }}</td>
        <td>{{ p.adresse }}</td>
        <td>{{ p.numero }}</td>
    </tr {% endfor %}>

</table>

</body>
