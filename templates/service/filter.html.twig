{% for service in services %}
    <div class="col-md-3 col-sm-6">
        <div class="ml-imageBox ml-imageBox--detailsHover ml-imageBox--hasShadow ml-imageBox--mb-40">
            <a class="ml-imageBox__anchor" href="{{ path('service',{'id':service.id}) }}">
                <div class="ml-imageBox-wrapper">
                    <div class="ml-imageBox-imgWrapper">
                        <img src="{{ asset('distClient/img-assets/logo.png') }} " alt="">
                    </div>
                </div><!--/.ml-imageBox-wrapper-->
                <div class="ml-imageBox-details ml-text--center">
                    <h4 class="ml-imageBox-title">{{ service.libelle }}</h4>
                    <h2 class="ml-imageBox-title">{{ service.prix }} TND</h2>
                </div><!--/.ml-imageBox-details-->
            </a><!--/.ml-imageBox__anchor-->
        </div><!--/.ml-imageBox-->
    </div><!--/.col-md-3 col-sm-6-->
{% endfor %}